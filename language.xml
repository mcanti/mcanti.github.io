	</plugin>

	<action name="ActivateLanguage">
	    if(lang=="ro",
	        set(plugin[flag_1].active,true);
					set(plugin[flag_1].visible,true);
	        set(plugin[flag_1].zorder,500);
	    );
	    if(lang=="en",
	        set(plugin[flag_2].active,true);
					set(plugin[flag_2].visible,true);
	        set(plugin[flag_2].zorder,500);
	    );
	    if(lang=="de",
	        set(plugin[flag_3].active,true);
					set(plugin[flag_3].visible,true);
	        set(plugin[flag_3].zorder,500);
	    );
	    if(lang=="fr",
	        set(plugin[flag_4].active,true);
					set(plugin[flag_4].visible,true);
	        set(plugin[flag_4].zorder,500);
	    );
	</action>

	<action name="TweenFlags">
	  if(%2,tween(plugin[%1].alpha,1,0.1,easeOutQuint);,tween(plugin[%1].alpha,0,0.1,easeOutQuint););
		tween(plugin[%1].x,get(flagx),0.5,easeOutQuint,set(plugin[%1].visible,%2));
	</action>
	<action name="OpenLanguages">
		set(flagx,get(skin_settings.thumb_language_right_padding));
		set(flag_zorder,500);
		for(set(j,1), j LT plugin[TourDefaultLanguage].count, inc(j),
			txtadd(thumbflag,'flag_',get(j));
			set(plugin[get(thumbflag)].visible,true);
			set(plugin[get(thumbflag)].enabled,true);
			if( plugin[get(thumbflag)].active
				,set(plugin[get(thumbflag)].active,false);
				,add(flagx,get(plugin[TourDefaultLanguage].padding));sub(flag_zorder,1);set(plugin[get(thumbflag)].zorder,get(flag_zorder));TweenFlags(get(thumbflag),true);
			);
			set(plugin[get(thumbflag)].onclick,CloseLanguages(get(name)));
		);
	</action>
	<action name="CloseLanguages">
		set(plugin[%1].active,true);
		set(plugin[%1].zorder,500);
		set(flagx,get(skin_settings.thumb_language_right_padding));
		set(flag_zorder,2000);
		for(set(j,1), j LT plugin[TourDefaultLanguage].count, inc(j),
			txtadd(thumbflag,'flag_',get(j));
			set(plugin[get(thumbflag)].visible,true);
			set(plugin[get(thumbflag)].enabled,true);
			TweenFlags(get(thumbflag),false);
			if( plugin[get(thumbflag)].active
				,set(plugin[TourDefaultLanguage].GetDefaultLang,get(plugin[get(thumbflag)].lang));
				txtadd(lang_var,'lang=',get(lang));
	            TweenFlags(get(thumbflag),true);
				if(skin_settings.activate_title == true,
					subtxt(get_scene_id,xml.scene,10,3);
					titke(get(get_scene_id));,
					copy(layer[skin_title].html, scene[get(xml.scene)].title );
				);
				,sub(flag_zorder,1);set(plugin[get(thumbflag)].zorder,get(flag_zorder));
			);

			set(plugin[get(thumbflag)].onclick,OpenLanguages());
		);
		events.dispatch(UpdateLanguageElements);
	</action>
	<action name="titke" >
		txtadd(layer[skin_title].html, get(plugin[get(plugin[TourDefaultLanguage].GetDefaultLang);].title) , ' - ', get(plugin[get(plugin[TourDefaultLanguage].GetDefaultLang);].scene_pano%1) );
	</action>
</krpano>
