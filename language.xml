	<plugin
		name="TourDefaultLanguage" keep="true" type="container" GetDefaultLang="en" count="5" padding="45" width="1" height="1" align="rightbottom" visible="false"
		>
		<plugin
				name="flag_1" url="%FIRSTXML%/assets/lang/flags-europe.png" crop="3|630|133|136" align="rightbottom" width="133" height="136" x="5" y="10" x_dest="50" scale="0.3" alpha="1.0" visible="false" enabled="true" keep="true" zorder="500" lang="ro" active="false"
				onhover="showtext(get(plugin[get(plugin[TourDefaultLanguage].GetDefaultLang);].LanguageRO), MouseHoverStyle);"
				onclick="OpenLanguages();"
				onloaded="set(y,get(skin_settings.thumb_language_top_padding));set(x,get(skin_settings.thumb_language_right_padding));"
				>
		</plugin>
		<plugin
				name="flag_2" url="%FIRSTXML%/assets/lang/flags-europe.png" crop="3|314|133|136" align="rightbottom" width="133" height="136" x="5" y="10" x_dest="50" scale="0.3" alpha="1.0" visible="false" enabled="true" keep="true" zorder="500" lang="en" active="false"
				onhover="showtext(get(plugin[get(plugin[TourDefaultLanguage].GetDefaultLang);].LanguageEN), MouseHoverStyle);"
				onclick="OpenLanguages();"
	            onloaded="set(y,get(skin_settings.thumb_language_top_padding));set(x,get(skin_settings.thumb_language_right_padding));"
				>
		</plugin>
